    "list.dropBackground": "#1A1919",
    "list.focusForeground": "#E2E4EB",
    "list.highlightForeground": "#16B553",
    "list.hoverBackground": "#1A1919",
    "list.hoverForeground": "#E2E4EB",
    "list.inactiveSelectionBackground": "#1A1919",
    "list.inactiveSelectionForeground": "#E2E4EB",
